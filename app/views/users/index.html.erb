<div class="row">
  <div class="col-xs-12">
    <div class="well">
      <div class="panel panel-info">
        <div class="panel-heading">
          <h1>Users</h1>
            <div class="form-group search-form">
              <%= render partial: 'application/search', locals: {item: "Users", path: users_path, id: "user-search"}%>
            </div>
          </div><!--close heading-->
          <div class="panel-body">
            <!-- <div class="well"> -->
              <table class="table table-striped catalogue-table">
              		<%= render partial: 'table', collection: alphabetized_by_last_name(@users), as: :user%>
              </table>
          </div><!--close body-->
        </div><!--close panel-->
      </div><!--close well-->
    </div><!--close column-->
  </div><!--close row-->
